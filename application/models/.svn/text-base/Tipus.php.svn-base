class Tipus
{
        private $_id;
        private $_nom;

        public function __construct($id, $nom)
        {

            $this->_id = $id;
            $this->_nom = $nom;

        }

        public function getNom()
        {
            return $this->_nom;
        }

        public function getId()
        {
            return $this->_id;
        }

        public function __toString()
        {
            return $this->_nom;
        }
}